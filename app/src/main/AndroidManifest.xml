<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ict376.tym.databaseview">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenuHost">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogBookDetailsHost"
            android:label="@string/Logbook" />
        <activity
            android:name=".LogBookHost"
            android:label="@string/Logbook" />
        <activity
            android:name=".CheckinHost"
            android:label="@string/CheckIn" />
        <activity android:name=".LevelupHost" />
        <activity android:name=".LevelUpPhotoHost" />
        <service
            android:name="BGMusic"></service>
    </application>

</manifest>